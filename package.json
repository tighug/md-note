{
  "name": "md-note",
  "version": "0.1.0",
  "description": "React Markdown Note",
  "main": "index.js",
  "author": "tighug",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build-main && yarn build-renderer",
    "build-main": "tsc --outDir build/main src/main/index.ts",
    "build-renderer": "webpack --config webpack.config.prod.js",
    "dev": "yarn build-main && yarn start-renderer-dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production electron build/main/index.js",
    "start-main-dev": "cross-env NODE_ENV=development electron build/main/index.js",
    "start-renderer-dev": "webpack-dev-server --config webpack.config.dev.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@reduxjs/toolkit": "^1.3.5",
    "clsx": "^1.1.0",
    "codemirror": "^5.52.2",
    "consola": "^2.11.3",
    "electron": "^8.2.1",
    "marked": "^0.8.2",
    "react": "^16.13.1",
    "react-codemirror2": "^7.1.0",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.0",
    "react-split-pane": "^0.1.91",
    "redux": "^4.0.5",
    "ress": "^2.0.4",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/codemirror": "^0.0.91",
    "@types/marked": "^0.7.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "devtron": "^1.4.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.2.0",
    "prettier": "^2.0.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
